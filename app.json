{
  "name": "whatsapp-bot (PINKVENOM)",
  "description": " Whatsapp bot.",
  "keywords": [
    "whatsapp bot"
  ],
  "repository": "https://github.com/Ayodya2007/re-bot-MD",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "session id",
      "required": true,
      "value": "12_02_H_E_R_M_I_T_MR_0D5K"
    },
    "SUDO": {
      "description": "admin",
      "required": true,
      "value": "null"
    },
    "MODE": {
      "description": "mode",
      "required": true,
      "value": "public"
    },
    "ALWAYS_ONLINE": {
      "description": "show bot number last seen as online",
      "required": true,
      "value": "true"
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": false
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": false
    },
    "LOG_MSG": {
      "description": "Show whatsapp msgs in log",
      "required": false,
      "value": "false"
    },
    "PREFIX": {
      "description": "prefix for bot. all so can be null or false multi prefix",
      "required": true,
      "value": "."
    },
    "BOT_INFO": {
      "description": "botname;ownername;ownernumber;image url",
      "required": true,
      "value": "🧚ᴘɪɴᴋ<ᴠᴇɴᴏᴍ🧚;🦄Aʏᴏᴅʏᴀ🦄;94772496127;https://telegra.ph/file/4f6930ac781d99e24c4b2.jpg"
    },
    "STICKER_DATA": {
      "description": "sticker package name",
      "required": true,
      "value": "B@A;𝛥yodya"
    },
    "AUDIO_DATA": {
      "description": "audio package name",
      "required": true,
      "value": "🧚ᴘɪɴᴋ_ᴠᴇɴᴏᴍ_ᴍᴅ🧚;🎀ᴀʸᵒᵈʸᴀ〠🎀;https://telegra.ph/file/2dae9decf20130a12a72e.mp4"
    },
    "READ_COMMAND": {
      "description": "Read all commands",
      "required": true,
      "value": "true"
    },
  "ERROR_MESSAGE": {
      "description": "get error alert",
      "required": false,
      "value": "true"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ]
}
